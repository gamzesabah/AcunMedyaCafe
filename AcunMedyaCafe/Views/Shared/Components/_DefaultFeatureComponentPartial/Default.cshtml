@model List<AcunMedyaCafe.Entities.Feature>

@foreach (var item in Model) { 
<section class="banner-area" id="home">
    <div class="container">
            <div class="row fullscreen d-flex align-items-center justify-content-start" style="text-align:left;">
            <div class="banner-content col-lg-7">
                <h4 class="text-white text-uppercase">@item.Subtitle</h4>
                <h1>
                    @item.Title
                </h1>
                <a href="#" class="primary-btn text-uppercase">Satın Al</a>
            </div>
        </div>
    </div>
</section>
}